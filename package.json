{
  "name": "beaf-ui",
  "version": "0.0.1",
  "description": "",
  "main": "dist/beaf-ui.common.js",
  "module": "dist/beaf-ui.esm.js",
  "unpkg": "dist/beaf-ui.js",
  "jsdelivr": "dist/beaf-ui.js",
  "sideEffects": false,
  "typings": "types/index.d.ts",
  "files": [
    "src/*.js",
    "dist/*.js",
    "types/*.d.ts"
  ],
  "keywords": [
    "vue",
    "rollup",
    "webpack",
    "ui",
    "components"
  ],
  "scripts": {
    "dev": "node examples/server.js",
    "dev:dist": "rollup -wm -c scripts/rollup.dev.config.js",
    "build": "node scripts/build.js",
    "lint": "eslint src test examples",
    "test": "npm run lint && npm run flow && npm run test:unit && npm run test:e2e && npm run test:types",
    "flow": "flow check",
    "test:unit": "jasmine JASMINE_CONFIG_PATH=test/unit/jasmine.json",
    "test:e2e": "node test/e2e/runner.js",
    "test:types": "tsc -p types/test",
    "docs": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "changelog": "conventional-changelog -p angular -r 2 -i CHANGELOG.md -s",
    "release": "bash scripts/release.sh"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "node scripts/verifyCommitMsg.js"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint --fix",
      "git add"
    ]
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow-vue": "^1.0.0",
    "buble": "^0.19.7",
    "eslint": "^5.16.0",
    "lint-staged": "^8.1.5",
    "rollup": "^1.10.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-embed-css": "^0.4.0",
    "rollup-plugin-eslint": "^5.1.0",
    "rollup-plugin-flow-no-whitespace": "^1.0.0",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-vue": "^5.0.0",
    "rollup-watch": "^4.3.1",
    "vue-template-compiler": "^2.6.10"
  }
}
